@model TaskManagement.Models.SubjectModels

<h1>Department Information</h1>

<form asp-action="SubjectCreate" asp-controller="Subjects" method="post" enctype="multipart/form-data">
    <label for="subject">Subject</label>
    <input type="text" id="subject" asp-for="Subject" required /><br /><br />

    <label for="subjectCode">Subject Code</label>
    <input type="text" id="subjectCode" asp-for="SubjectCode" required /><br /><br />

    <input type="submit" value="Submit" />
</form>